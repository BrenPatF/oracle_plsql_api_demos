{
   "meta":{
      "title":"TT_Emp_WS.tt_AIP_Save_Emps",
      "inp":{
         "Employee":[
            "Name",
            "Email",
            "Job",
            "Salary"
         ]
      },
      "out":{
         "Employee":[
            "Employee id",
            "Name",
            "Email",
            "Job",
            "Salary"
         ],
         "Output array":[
            "Employee id",
            "Description"
         ],
         "Exception":[
            "Error message"
         ]
      }
   },
   "scenarios":{
      "1 valid record":{
         "inp":{
            "Employee":[
               "LN 1|EM 1|IT_PROG|1000"
            ]
         },
         "out":{
            "Employee":[
               "1|LN 1|EM 1|IT_PROG|1000"
            ],
            "Output array":[
               "1|LIKE /^[A-Z -]+[A-Z]$/"
            ],
            "Exception":[
            ]
         }
      },
      "1 invalid job id":{
         "inp":{
            "Employee":[
               "LN 2|EM 2|NON_JOB|1500"
            ]
         },
         "out":{
            "Employee":[
            ],
            "Output array":[
               "0|ORA-02291: integrity constraint (.) violated - parent key not found"
            ],
            "Exception":[
            ]
         }
      },
      "1 invalid number":{
         "inp":{
            "Employee":[
               "LN 3|EM 3|IT_PROG|2000x"
            ]
         },
         "out":{
            "Employee":[
            ],
            "Output array":[
            ],
            "Exception":[
               "ORA-06502: PL/SQL: numeric or value error: character to number conversion error"
            ]
         }
      },
      "2 valid records, 1 invalid job id (2 deliberate errors)":{
         "inp":{
            "Employee":[
               "LN 4|EM 4|IT_PROG|3000",
               "LN 5|EM 5|NON_JOB|4000",
               "LN 6|EM 6|IT_PROG|5000"
            ]
         },
         "out":{
            "Employee":[
               "3|LN 4|EM 4|IT_PROG|1000",
               "5|LN 6|EM 6|IT_PROG|5000",
               "5|LN 6|EM 6|IT_PROG|5000"
            ],
            "Output array":[
               "3|LIKE /^[A-Z -]+[A-Z]$/",
               "0|ORA-02291: integrity constraint (.) violated - parent key not found",
               "5|LIKE /^[A-Z -]+[A-Z]$/"
            ],
            "Exception":[
            ]
         }
      }
   }
}